#include <stdio.h>

int main()
{
    char c[26];
    int i;
    
    for(i=0;i<26;i++) {
        c[i]='A'+(char)i;
    }
    for(i=25;i>=0;i--) {
        printf("%c ", c[i]);
    }
    return 0;
}
/*
